<!DOCTYPE etl SYSTEM "http://scriptella.javaforge.com/dtd/etl.dtd">
<etl>
    <properties>
        n=10
    </properties>
    <connection driver="jexl"/>
    <query>
        i=0;
        while(i lt n) {
            i=i+1;
            query.next();
        }
        <script>
            callback.notify(i);
        </script>
    </query>
    <script>
        <!-- Using scriptella class: namespace function -->
        class:forName('scriptella.driver.jexl.JexlDriverITest').runStatic(n == 10 ? 'ok' : 'error');
    </script>
</etl>